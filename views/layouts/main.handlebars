      <div class='input-field col s12'>
        <input id="login-email"
          placeholder='Placeholder'
          type='text'
          class='validate'
        />
        <label for='login-email'>Email</label>
      </div>
    </div>
    <div class='row'>
      <div class='input-field col s12'>
         <label for='password'>Password</label>
        <input id='login-password' type='password' />
       
      </div>
    </div>
      <button type="submit" class="login-form waves-effect waves-green btn-flat">Submit</button>
  </form>
</div>
    </div>
    <div class="modal-footer">
      <a  id="login-submit" class="modal-close waves-effect waves-green btn-flat">Submit</a>
    </div>
  </div>
      <a href="#" class="brand-logo center">Wharf Rats</a>
